    "description": "Biblioteca para gerar formulários estilizados com bootstrap e Laravel Forms",
    "type": "library",
    "require": {
        "laravel/framework": "^5.5",
        "laravelcollective/html": "^5.5"
    },
    "authors": [
        {
            "name": "Kdym Soluções",
            "email": "kdymsolucoes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "KdymSolucoes\\BootstrapForms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KdymSolucoes\\BootstrapForms\\BootstrapFormsServiceProvider"
            ]
        }
    }
}
