{
    "name": "agenciamaior/bootstrap_forms",
    "description": "Biblioteca para gerar formulários estilizados com bootstrap e Laravel Forms",
    "type": "library",
    "require": {
        "laravel/framework": "5.*",
        "laravelcollective/html": ">=5.5"
    },
    "authors": [
        {
            "name": "Agência Maior",
            "email": "alexandre.sottani@agenciamaior.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "AgenciaMaior\\BootstrapForms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenciaMaior\\BootstrapForms\\BootstrapFormsServiceProvider"
            ]
        }
    }
}
